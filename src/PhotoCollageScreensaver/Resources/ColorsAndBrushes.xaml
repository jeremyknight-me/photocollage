<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PhotoCollageScreensaver.Resources">
    <Color x:Key="DarkGray">#ff222222</Color>
    <Color x:Key="GrayEEE">#FFeeeeee</Color>
    <SolidColorBrush x:Key="BrushGrayEEE" Color="{StaticResource GrayEEE}"/>
    <SolidColorBrush x:Key="DarkGrayBrush" Color="{StaticResource DarkGray}" />
</ResourceDictionary>